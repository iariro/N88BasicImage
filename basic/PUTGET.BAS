1000 ' GET/PUT
1010 COLOR 0,0,0,0,2
1020 SCREEN 3,0,0,1
1030 CLS 3
1040 DIM IMG%((2*2*4+4)/2)
1050 INPUT "put or get or ldfile? ", C$
1060 IF C$="put" THEN GOSUB *PUTTEST
1070 IF C$="get" THEN GOSUB *GETTEST
1080 IF C$="ldfile" THEN GOSUB *LDFILE
1090 END
1100 '
1110 *GETTEST
1120 'FOR I=0 TO 15
1130 'PSET(I, 0),I
1140 'PSET(I, 1),15-I
1150 'NEXT I
1160 PSET(0, 1),1
1170 'PSET(15, 0),1
1180 '
1190 GET(0,0)-(15,1),IMG%
1200 FOR I=0 TO 9
1210 PRINT HEX$(IMG%(I));",";
1220 NEXT I
1230 RETURN
1240 '
1250 *PUTTEST
1260 FOR I=0 TO 10
1270 READ J$
1280 IMG%(I) = VAL("&H"+J$)
1290 NEXT I
1300 PUT(100,100),IMG%
1310 RETURN
1320 '
1330 DATA 0010,0002,5555,3333,0F0F,FF00,AAAA,CCCC,F0F0,00FF,0000
1340 '
1350 *LDFILE
1360 INPUT "file name? ", FILENAME$
1370 OPEN FILENAME$ FOR INPUT AS #1
1371 FOR I=0 TO 15
1372 INPUT #1, LN$
1373 color=(i, VAL("&h" + LN$))
1374 next
1380 INPUT #1, LN$
1390 X = VAL("&h" + LN$)
1400 INPUT #1, LN$
1410 Y = VAL("&h" + LN$)
1420 PRINT X;",";Y
1430 DIM IMG2%((((X+7)\8)*Y*4+4)/2)
1440 IMG2%(0) = X
1450 IMG2%(1) = Y
1460 CNT=2
1470 WHILE EOF(1)=0
1480  INPUT #1, LN$
1490  IF LN$="" THEN 1540
1500  IF CNT >= (((X+7)\8)*Y*4+4)/2 THEN 1530
1510  IMG2%(CNT) = VAL("&h" + LN$)
1520  IF CNT MOD 500 = 0 THEN PRINT CNT; IMG2%(CNT)
1530  CNT = CNT + 1
1540 WEND
1550 CLOSE #1
1560 PUT(400, 100),IMG2%
1570 RETURN
